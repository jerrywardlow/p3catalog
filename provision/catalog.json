{
    "variables": {
        "aws_access_key": "{{env `AWS_KEY`}}",
        "aws_secret_key": "{{env `AWS_SECRET`}}"
    },
    "builders": [{
        "type": "amazon-ebs",
        "access_key": "{{user `aws_access_key`}}",
        "secret_key": "{{user `aws_secret_key`}}",
        "region": "us-west-2",
        "source_ami": "ami-962fedf6",
        "instance_type": "t2.micro",
        "ssh_username": "ubuntu",
        "ami_name": "p3catalog {{timestamp}}"
    }],
    "provisioners": [{
        "type": "shell",
        "inline": [ "sleep 30" ]
    }]
}
